# Phase YAML -> Schedule CSV\
# ---------------------------\
\
def generate_schedule_from_phases(phase_yaml_path: str, Q: int, quarterly_cap_pct: float) -> pd.DataFrame:\
    """\
    Phase YAML format:\
    phases:\
      - name: Phase 1\
        start_quarter: 1\
        total_pct: 0.10               # fraction of TOTAL supply unlocked over the phase\
        duration_quarters: 4          # optional; if omitted, computed min to fit cap\
        buckets:                      # weights across buckets (sum to 1.0)\
          Research: 0.40\
          Ecosystem: 0.20\
          Team: 0.10\
          Partners: 0.10\
          Treasury: 0.10\
          Liquidity: 0.10\
\
    Returns a DataFrame with Quarter + BUCKETS (percent of total per quarter).\
    """\
    with open(phase_yaml_path, "r") as f:\
        y = yaml.safe_load(f)\
    phases = y.get("phases", [])\
    if not phases:\
        raise ValueError("phase YAML has no 'phases' list")\
\
    rows = [\{"Quarter": f"Q\{i + 1\}", **\{b: 0.0 for b in BUCKETS\}\} for i in range(Q)]\
    df = pd.DataFrame(rows)\
\
    for ph in phases:\
        name = ph.get("name", "Phase")\
        start = int(ph["start_quarter"])\
        total_pct = float(ph["total_pct"])\
        weights = ph.get("buckets", \{\})\
        w = np.array([float(weights.get(b, 0.0)) for b in BUCKETS], dtype=float)\
        if w.sum() <= 0:\
            raise ValueError(f"\{name\}: bucket weights must sum > 0")\
        w = w / w.sum()\
\
        dur = ph.get("duration_quarters", None)\
        if dur is None:\
            dur = math.ceil(total_pct / quarterly_cap_pct)\
        per_q_total = total_pct / dur\
        if per_q_total > quarterly_cap_pct:\
            dur = math.ceil(total_pct / quarterly_cap_pct)\
            per_q_total = total_pct / dur\
\
        for i in range(dur):\
            q_index = start - 1 + i\
            if q_index >= Q:\
                break\
            for bi, b in enumerate(BUCKETS):\
                df.at[q_index, b] += per_q_total * w[bi]\
\
    return df\
\
\
def enforce_global_quarterly_cap(sched_df: pd.DataFrame, quarterly_cap_pct: float) -> pd.DataFrame:\
    """\
    If any quarter\'92s total % exceeds cap, spill the excess forward (preserving bucket ratios).\
    """\
    df = sched_df.copy()\
    Q = len(df)\
    cols = BUCKETS[:]\
    totals = df[cols].sum(axis=1).values\
\
    for i in range(Q):\
        if totals[i] > quarterly_cap_pct + 1e-12:\
            row = df.loc[i, cols].astype(float)\
            s = row.sum()\
            if s > 0:\
                # scale down to cap\
                df.loc[i, cols] = row * (quarterly_cap_pct / s)\
                excess = s - quarterly_cap_pct\
                # spill to next quarter (preserving proportions)\
                if i + 1 < Q:\
                    next_row = df.loc[i + 1, cols].astype(float)\
                    df.loc[i + 1, cols] = next_row + (row / s) * excess\
            totals = df[cols].sum(axis=1).values\
\
    return df\
\
# ===== Extra helpers for richer output =====\
\
def pct(x: float) -> str:\
    return f"\{x*100:0.2f\}%"\
\
def ansi(color: str, s: str) -> str:\
    codes = \{"red":"\\033[31m", "yellow":"\\033[33m", "green":"\\033[32m", "reset":"\\033[0m"\}\
    return f"\{codes.get(color,'')\}\{s\}\{codes['reset']\}"\
\
def bucket_totals_pct(sched_df: pd.DataFrame) -> dict:\
    totals = \{b: float(sched_df[b].sum()) for b in BUCKETS\}\
    totals["Total"] = float(sum(totals[b] for b in BUCKETS))\
    return totals\
\
def find_min_treasury_point(out: pd.DataFrame) -> tuple[float, str]:\
    idx = int(out["Treasury_Cash_USD"].idxmin())\
    return float(out["Treasury_Cash_USD"].iloc[idx]), str(out["Quarter"].iloc[idx])\
\
def first_deflationary_quarter(out: pd.DataFrame) -> str:\
    neg = out.index[out["Net_Issuance_SAIT"] < 0]\
    return str(out.loc[neg[0], "Quarter"]) if len(neg) else "\'97"\
\
def overhang_count(out: pd.DataFrame, threshold: float) -> int:\
    return int((out["Overhang_vs_Float"] > threshold).sum())\
