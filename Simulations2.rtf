def consolidated_recommendations(kpis_list: list[dict]) -> list[str]:\
    recs = []\
    # Flags across scenarios\
    any_no_defl = any(k["deflationary_quarters"] == 0 for k in kpis_list)\
    any_overhang_hi = any(k["max_overhang_vs_float"] >= 0.50 for k in kpis_list)\
    any_runway_short = any((isinstance(k["min_runway_quarters"], int) and k["min_runway_quarters"] < 6) for k in kpis_list)\
    # Recommendations\
    if any_no_defl:\
        recs.append("Increase burn intensity (raise burn_share) and/or adoption (actions growth, fee_per_action) to create at least 1\'962 deflationary quarters by Year 2.")\
    if any_overhang_hi:\
        recs.append("Reduce early unlocks (longer durations, stagger phases) or slightly increase initial float to bring Max Overhang/Float below 40%.")\
    if any_runway_short:\
        recs.append("Extend treasury runway (raise treasury_share or reduce ops spend) to \uc0\u8805  8\'9612 quarters.")\
    # Always useful\
    recs.append("Publish the cap-compliant unlock curve and overhang metric alongside governance proposals to set expectations.")\
    recs.append("Add a burn floor (e.g., 2M SAIT/q) financed from treasury early on; remove when usage burns dominate.")\
    return recs\
