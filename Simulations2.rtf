def simulate_one_supply(\
    total_supply: int,\
    cfg: dict,\
    sched_pct: pd.DataFrame,\
    price_series: Optional[np.ndarray]\
) -> pd.DataFrame:\
    Q = int(cfg["quarters"])\
    # schedule (% of total) -> SAIT\
    unlocks_sait = sched_pct.copy()\
    for b in BUCKETS:\
        unlocks_sait[b] = (sched_pct[b].astype(float) * total_supply).round(0)\
    unlocks_sait["Total_Unlocks"] = unlocks_sait[BUCKETS].sum(axis=1)\
\
    # Quarterly cap (3 * monthly cap) \'97 for reporting only (global cap already enforced if chosen)\
    quarterly_cap_pct = 3.0 * float(cfg["max_unlock_cap_per_month"])\
    q_cap_units = quarterly_cap_pct * total_supply\
    cap_flags = (unlocks_sait["Total_Unlocks"] > q_cap_units)\
\
    # Usage & fees\
    actions = geometric_series(Q, cfg["base_actions_q1"], cfg["actions_growth_qoq"])\
    fee_revenue = actions * float(cfg["fee_per_action_usd"])\
\
    # Price path\
    if price_series is not None:\
        prices = price_series\
    else:\
        prices = geometric_series(Q, cfg["price_q1_usd"], cfg["price_growth_qoq"])\
\
    # Splits\
    burn_share = float(cfg["burn_share"])\
    stakers_share = float(cfg["stakers_share"])\
    treasury_share = float(cfg["treasury_share"])\
    if abs((burn_share + stakers_share + treasury_share) - 1.0) > 1e-6:\
        raise ValueError("burn+stakers+treasury must sum to 1.0")\
\
    # Usage-coupled burns\
    burns_usd_usage = fee_revenue * burn_share\
    burns_sait_usage = np.where(prices > 0, burns_usd_usage / prices, 0.0)\
\
    # Burn floor (SAIT)\
    burn_floor_sait = float(cfg.get("burn_floor_sait_per_q", 0.0))\
    finance_mode = str(cfg.get("burn_floor_finance", "treasury")).lower()  # "treasury" or "ignore"\
\
    # Final burns with floor\
    burns_sait = np.maximum(burns_sait_usage, burn_floor_sait)\
    burns_usd = burns_sait * prices\
\
    # Treasury inflow (adjust if extra burn financed from treasury)\
    treasury_inflow_usd_nominal = fee_revenue * treasury_share\
    extra_burn_sait = np.maximum(0.0, burns_sait - burns_sait_usage)\
    extra_burn_usd = extra_burn_sait * prices\
    if finance_mode == "treasury":\
        treasury_inflow_usd = np.maximum(0.0, treasury_inflow_usd_nominal - extra_burn_usd)\
    else:\
        treasury_inflow_usd = treasury_inflow_usd_nominal\
\
    # Stakers nominal payout\
    stakers_payout_usd_nominal = fee_revenue * stakers_share\
\
    # Trackers\
    starting_circ = np.zeros(Q)\
    ending_circ = np.zeros(Q)\
    net_issuance = np.zeros(Q)\
    overhang_2q = np.zeros(Q)\
    overhang_pct = np.zeros(Q)\
\
    treasury_cash = np.zeros(Q)\
    ops_spend = np.full(Q, float(cfg["ops_spend_per_q_usd"]), dtype=float)\
\
    staked_pct = float(cfg["staked_pct_of_circ"])\
    target_apy = float(cfg["target_staker_apy"])\
    realized_staker_apy = np.zeros(Q)\
    staked_value_usd = np.zeros(Q)\
    stakers_payout_usd = np.zeros(Q)\
\
    initial_circ = total_supply * float(cfg["initial_circulating_pct"])\
\
    for i in range(Q):\
        starting_circ[i] = initial_circ if i == 0 else ending_circ[i - 1]\
        unlocks_q = float(unlocks_sait.loc[i, "Total_Unlocks"])\
        burns_q_sait = burns_sait[i]\
\
        ending_circ[i] = starting_circ[i] + unlocks_q - burns_q_sait\
        net_issuance[i] = unlocks_q - burns_q_sait\
\
        # Overhang next 2Q\
        if i + 1 < Q:\
            j2 = min(i + 2, Q - 1)\
            over = unlocks_sait.loc[i + 1:j2, "Total_Unlocks"].sum()\
        else:\
            over = 0.0\
        overhang_2q[i] = over\
        overhang_pct[i] = (over / starting_circ[i]) if starting_circ[i] > 0 else 0.0\
\
        # Staker payouts (fee-funded & APY-capped)\
        avg_circ = 0.5 * (starting_circ[i] + ending_circ[i])\
        staked_val = avg_circ * staked_pct * prices[i]\
        staked_value_usd[i] = staked_val\
        cap_quarterly = target_apy / 4.0 * staked_val\
        stakers_payout_usd[i] = min(stakers_payout_usd_nominal[i], cap_quarterly)\
        realized_staker_apy[i] = (stakers_payout_usd[i] / staked_val * 4.0) if staked_val > 0 else 0.0\
\
        # Treasury cash cumulative\
        treasury_cash[i] = (\
            float(cfg["starting_treasury_cash_usd"])\
            + float(treasury_inflow_usd[: i + 1].sum())\
            - float(ops_spend[: i + 1].sum())\
        )\
\
    out = pd.DataFrame(\{\
        "Quarter": [f"Q\{i + 1\}" for i in range(Q)],\
        "Starting_Circ_SAIT": starting_circ.round(0),\
        "Price_USD": prices,\
        "Fee_Actions": actions.round(0),\
        "Fee_Revenue_USD": fee_revenue,\
        "Burns_USD": burns_usd,\
        "Burns_SAIT": burns_sait.round(0),\
        "Unlocks_SAIT": unlocks_sait["Total_Unlocks"].values,\
        "Ending_Circ_SAIT": ending_circ.round(0),\
        "Net_Issuance_SAIT": net_issuance.round(0),\
        "Overhang_2Q_SAIT": overhang_2q.round(0),\
        "Overhang_vs_Float": overhang_pct,\
        "Staked_Value_USD": staked_value_usd,\
        "Stakers_Payout_USD": stakers_payout_usd,\
        "Realized_Staker_APY": realized_staker_apy,\
        "Treasury_Cash_USD": treasury_cash,\
        "Unlocks_Cap_Breached": (unlocks_sait["Total_Unlocks"] > q_cap_units).values\
    \})\
    out["Cumulative_Burns_SAIT"] = out["Burns_SAIT"].cumsum().round(0)\
    out["Cumulative_Burns_%Total"] = out["Cumulative_Burns_SAIT"] / float(total_supply)\
        # --- Fundraising tiers (optional) ---\
    sales_yaml_path = "sims/sales.yaml"\
    sales = []\
    try:\
        sales = load_sales_yaml(sales_yaml_path, Q)\
    except FileNotFoundError:\
        sales = []\
\
    sale_amounts = np.zeros(Q)\
    sale_proceeds = np.zeros(Q)\
    for s in sales:\
        i = s["quarter"] - 1\
        sale_amounts[i] += s["amount_sait"]\
        sale_proceeds[i] += s["amount_sait"] * s["price_usd"]\
\
    out["Crowdfund_Sold_SAIT"] = sale_amounts\
    out["Crowdfund_Proceeds_USD"] = sale_proceeds\
\
    # Optional: treat crowdfund proceeds as treasury cash (ring-fenced)\
    out["Treasury_Cash_USD"] = out["Treasury_Cash_USD"] + np.cumsum(sale_proceeds)\
\
\
    return out\
\
\
def kpis_from_out(\
    out: pd.DataFrame,\
    total_supply: int,\
    cfg: dict,\
    sched_pct: pd.DataFrame,\
    quarterly_cap_pct: float\
) -> dict:\
    init_float = total_supply * float(cfg["initial_circulating_pct"])\
    per_q_totals = sched_pct[BUCKETS].sum(axis=1).values\
\
    # Staker APY stats\
    apy = out["Realized_Staker_APY"].to_numpy(dtype=float)\
    avg_apy = float(apy.mean()) if len(apy) else 0.0\
    min_apy = float(apy.min()) if len(apy) else 0.0\
    max_apy = float(apy.max()) if len(apy) else 0.0\
\
    final_circ = int(out["Ending_Circ_SAIT"].iloc[-1])\
    final_circ_pct = final_circ / float(total_supply)\
\
    # min treasury balance quarter\
    min_bal, min_bal_q = find_min_treasury_point(out)\
\
    return \{\
        "total_supply": int(total_supply),\
        "initial_float": int(init_float),\
        "final_circulating": final_circ,\
        "final_circ_pct": pct(final_circ_pct),\
        "cum_burn_sait": int(out["Burns_SAIT"].sum()),\
        "cum_burn_pct_total": float(out["Burns_SAIT"].sum() / float(total_supply)),\
        "deflationary_quarters": int((out["Net_Issuance_SAIT"] < 0).sum()),\
        "first_deflation_q": first_deflationary_quarter(out),\
        "min_runway_quarters": int(np.argmax(out["Treasury_Cash_USD"] < 0)) if (out["Treasury_Cash_USD"] < 0).any() else math.inf,\
        "treasury_min_balance": float(min_bal),\
        "treasury_min_balance_q": min_bal_q,\
        "max_overhang_vs_float": float(out["Overhang_vs_Float"].max() if len(out) > 0 else 0.0),\
        "peak_unlock_pct_per_q": float(per_q_totals.max()),\
        "cap_breach_any": bool((per_q_totals > quarterly_cap_pct + 1e-12).any()),\
        "avg_staker_apy": float(avg_apy),\
        "min_staker_apy": float(min_apy),\
        "max_staker_apy": float(max_apy),\
        "total_unlocked_pct_16q": float(per_q_totals.sum()),\
    \}\
\
\
def pretty_print_kpis(kpis_list: list[dict]) -> None:\
    headers = [\
        "TotalSupply","InitFloat","FinalCirc","FinalCirc(%)",\
        "CumBurn(SAIT)","CumBurn(%)","DeflQ","RunwayQ(min)",\
        "PeakUnl/q","MaxOverhang/Float","AvgAPY","CapBreach"\
    ]\
    rows = []\
    for k in kpis_list:\
        over = k["max_overhang_vs_float"]\
        over_str = f"\{over*100:0.1f\}%"\
        if over >= 0.50:\
            over_str = ansi("red", over_str)\
        elif over >= 0.40:\
            over_str = ansi("yellow", over_str)\
        else:\
            over_str = ansi("green", over_str)\
\
        runway = k["min_runway_quarters"]\
        runway_str = str(runway)\
\
        rows.append([\
            f"\{k['total_supply']:,\}",\
            f"\{k['initial_float']:,\}",\
            f"\{k['final_circulating']:,\}",\
            k["final_circ_pct"],\
            f"\{k['cum_burn_sait']:,\}",\
            f"\{k['cum_burn_pct_total']*100:0.2f\}%",\
            k["deflationary_quarters"],\
            runway_str,\
            f"\{k['peak_unlock_pct_per_q']*100:0.2f\}%",\
            over_str,\
            f"\{k['avg_staker_apy']*100:0.2f\}%",\
            "YES" if k["cap_breach_any"] else "no",\
        ])\
\
    col_widths = [max(len(str(x)) for x in [h] + [r[i] for r in rows]) for i, h in enumerate(headers)]\
    sep = "+" + "+".join("-" * (w + 2) for w in col_widths) + "+"\
    print(sep)\
    print("| " + " | ".join(h.ljust(col_widths[i]) for i, h in enumerate(headers)) + " |")\
    print(sep)\
    for r in rows:\
        print("| " + " | ".join(str(r[i]).ljust(col_widths[i]) for i in range(len(headers))) + " |")\
    print(sep)\
\
