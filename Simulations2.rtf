BUCKETS = ["Research", "Ecosystem", "Team", "Partners", "Treasury", "Liquidity"]\
\
\
# ---------------------------\
# Helpers\
# ---------------------------\
\
def geometric_series(q: int, start: float, growth: float) -> np.ndarray:\
    """Q-length geometric series: x0=start; x_t = x_\{t-1\}*(1+growth)."""\
    arr = [float(start)]\
    for _ in range(1, q):\
        arr.append(arr[-1] * (1.0 + float(growth)))\
    return np.array(arr, dtype=float)\
\
\
def load_inputs(config_path: str) -> dict:\
    with open(config_path, "r") as f:\
        cfg = yaml.safe_load(f)\
    return cfg\
\
\
def load_schedule_csv(path: str) -> pd.DataFrame:\
    sched = pd.read_csv(path)\
    required_cols = ["Quarter"] + BUCKETS\
    missing = [c for c in required_cols if c not in sched.columns]\
    if missing:\
        raise ValueError(f"unlock schedule missing columns: \{missing\}")\
    if (sched[BUCKETS] < 0).any().any():\
        raise ValueError("unlock schedule contains negative percentages")\
    return sched\
\
\
def load_price_csv(path: str, Q: int) -> np.ndarray:\
    df = pd.read_csv(path)\
    if "Price_USD" not in df.columns:\
        # allow single-column CSV\
        first = df.columns[0]\
        df = df.rename(columns=\{first: "Price_USD"\})\
    series = df["Price_USD"].astype(float).values\
    if len(series) < Q:\
        raise ValueError(f"price_csv has \{len(series)\} rows but needs \{Q\}")\
    return series[:Q]\
\
def load_sales_yaml(path: str, Q: int) -> list[dict]:\
    with open(path, "r") as f:\
        y = yaml.safe_load(f) or \{\}\
    tiers = y.get("tiers", [])\
    packed = []\
    for t in tiers:\
        q = t["quarter"]\
        if isinstance(q, str) and "-" in q:\
            a, b = q.split("-")\
            qs = list(range(int(a), int(b)+1))\
        else:\
            qs = [int(q)]\
        amt = int(t["amount_sait"])\
        price = t.get("price_usd", None)\
        prange = t.get("price_usd_range", None)\
        if prange:\
            lo, hi = float(prange[0]), float(prange[1])\
            price = (lo + hi) / 2.0  # choose mid by default; you can parametrize\
        assert price is not None\
        for qi in qs:\
            if 1 <= qi <= Q:\
                packed.append(\{"quarter": qi, "amount_sait": amt // len(qs), "price_usd": float(price)\})\
    return packed\
\
