#!/usr/bin/env python3\
import argparse, os, shutil, yaml, subprocess, json\
\
BASE_CFG = "sims/config.yaml"\
PHASES = "sims/phases.yaml"\
PRICES = "sims/prices.csv"\
\
def deep_merge(a, b):\
    out = dict(a)\
    for k, v in b.items():\
        if isinstance(v, dict) and isinstance(out.get(k), dict):\
            out[k] = deep_merge(out[k], v)\
        else:\
            out[k] = v\
    return out\
\
def main():\
    ap = argparse.ArgumentParser()\
    ap.add_argument("--profiles", default="sims/scenario_profiles.yaml")\
    ap.add_argument("--outroot", default="sims/outputs_scenarios")\
    args = ap.parse_args()\
\
    os.makedirs(args.outroot, exist_ok=True)\
    base_cfg = yaml.safe_load(open(BASE_CFG))\
    profs = yaml.safe_load(open(args.profiles))["profiles"]\
\
    for p in profs:\
        name = p["name"]\
        overrides = p.get("overrides", \{\})\
        workdir = os.path.join(args.outroot, name)\
        os.makedirs(workdir, exist_ok=True)\
\
        cfg = deep_merge(base_cfg, overrides)\
        cfg_path = os.path.join(workdir, "config.yaml")\
        with open(cfg_path, "w") as f: yaml.safe_dump(cfg, f, sort_keys=False)\
\
        # copy shared inputs\
        shutil.copyfile(PHASES, os.path.join(workdir, "phases.yaml"))\
        shutil.copyfile(PRICES, os.path.join(workdir, "prices.csv"))\
\
        # run sim\
        cmd = [\
            "python", "sims/sait_models.py",\
            "--config", cfg_path,\
            "--phase_yaml", os.path.join(workdir, "phases.yaml"),\
            "--price_csv", os.path.join(workdir, "prices.csv"),\
            "--outdir", workdir,\
            "--verbose"\
        ]\
        print("==>", name, " ".join(cmd))\
        subprocess.run(cmd, check=True)\
\
    # optional: build a cross-profile index\
    index_rows = []\
    for p in profs:\
        workdir = os.path.join(args.outroot, p["name"])\
        kpi_csv = os.path.join(workdir, "kpis_summary.csv")\
        if os.path.exists(kpi_csv):\
            index_rows.append(\{"profile": p["name"], "kpi_csv": kpi_csv\})\
    with open(os.path.join(args.outroot, "INDEX.json"), "w") as f:\
        json.dump(index_rows, f, indent=2)\
\
if __name__ == "__main__":\
    main()\
\
