#!/usr/bin/env python3\
import argparse, os, shutil, yaml, subprocess, json\
\
BASE_CFG = "sims/config.yaml"\
PHASES = "sims/phases.yaml"\
PRICES = "sims/prices.csv"\
\
def deep_merge(a, b):\
    out = dict(a)\
    for k, v in b.items():\
        if isinstance(v, dict) and isinstance(out.get(k), dict):\
            out[k] = deep_merge(out[k], v)\
        else:\
            out[k] = v\
    return out\
\
def main():\
    ap = argparse.ArgumentParser()\
    ap.add_argument("--profiles", default="sims/scenario_profiles.yaml")\
    ap.add_argument("--outroot", default="sims/outputs_scenarios")\
    args = ap.parse_args()\
\
    os.makedirs(args.outroot, exist_ok=True)\
    base_cfg = yaml.safe_load(open(BASE_CFG))\
    profs = yaml.safe_load(open(args.profiles))["profiles"]\
\
    for p in profs:\
        name = p["name"]\
        overrides = p.get("overrides", \{\})\
        workdir = os.path.join(args.outroot, name)\
        os.makedirs(workdir, exist_ok=True)\
\
        cfg = deep_merge(base_cfg, overrides)\
        cfg_path = os.path.join(workdir, "config.yaml")\
        with open(cfg_path, "w") as f: yaml.safe_dump(cfg, f, sort_keys=False)\
\
        # copy shared inputs\
        shutil.copyfile(PHASES, os.path.join(workdir, "phases.yaml"))\
        shutil.copyfile(PRICES, os.path.join(workdir, "prices.csv"))\
\
        # run sim\
        cmd = [\
            "python", "sims/sait_models.py",\
            "--config", cfg_path,\
            "--phase_yaml", os.path.join(workdir, "phases.yaml"),\
            "--price_csv", os.path.join(workdir, "prices.csv"),\
            "--outdir", workdir,\
            "--verbose"\
        ]\
        print("==>", name, " ".join(cmd))\
        subprocess.run(cmd, check=True)\
\
    # optional: build a cross-profile index\
    index_rows = []\
    for p in profs:\
        workdir = os.path.join(args.outroot, p["name"])\
        kpi_csv = os.path.join(workdir, "kpis_summary.csv")\
        if os.path.exists(kpi_csv):\
            index_rows.append(\{"profile": p["name"], "kpi_csv": kpi_csv\})\
    with open(os.path.join(args.outroot, "INDEX.json"), "w") as f:\
        json.dump(index_rows, f, indent=2)\
\
if __name__ == "__main__":\
    main()\
\
#!/usr/bin/env python3\
"""\
SAIT Tokenomics Simulator (CLI)\
- Reads a YAML config and either:\
  a) per-quarter unlock schedule CSV (percent of total), or\
  b) Phase YAML and generates a cap-compliant schedule.\
- Optionally overrides price path with a per-quarter price CSV.\
- Sweeps over multiple total-supply values.\
- Prints enriched KPI table, plus writes detailed CSVs/MD/JSON to outputs.\
\
Usage:\
  python3 sait_models.py \\\
    --config config.yaml \\\
    --phase_yaml phases.yaml \\\
    --price_csv prices.csv \\\
    --outdir outputs \\\
    --verbose\
"""\
\
import argparse\
import json\
import math\
import os\
from typing import Optional\
\
import numpy as np\
import pandas as pd\
import yaml\
\
BUCKETS = ["Research", "Ecosystem", "Team", "Partners", "Treasury", "Liquidity"]\
\
\
# ---------------------------\
# Helpers\
# ---------------------------\
\
def geometric_series(q: int, start: float, growth: float) -> np.ndarray:\
    """Q-length geometric series: x0=start; x_t = x_\{t-1\}*(1+growth)."""\
    arr = [float(start)]\
    for _ in range(1, q):\
        arr.append(arr[-1] * (1.0 + float(growth)))\
    return np.array(arr, dtype=float)\
\
\
def load_inputs(config_path: str) -> dict:\
    with open(config_path, "r") as f:\
        cfg = yaml.safe_load(f)\
    return cfg\
\
\
def load_schedule_csv(path: str) -> pd.DataFrame:\
    sched = pd.read_csv(path)\
    required_cols = ["Quarter"] + BUCKETS\
    missing = [c for c in required_cols if c not in sched.columns]\
    if missing:\
        raise ValueError(f"unlock schedule missing columns: \{missing\}")\
    if (sched[BUCKETS] < 0).any().any():\
        raise ValueError("unlock schedule contains negative percentages")\
    return sched\
\
\
def load_price_csv(path: str, Q: int) -> np.ndarray:\
    df = pd.read_csv(path)\
    if "Price_USD" not in df.columns:\
        # allow single-column CSV\
        first = df.columns[0]\
        df = df.rename(columns=\{first: "Price_USD"\})\
    series = df["Price_USD"].astype(float).values\
    if len(series) < Q:\
        raise ValueError(f"price_csv has \{len(series)\} rows but needs \{Q\}")\
    return series[:Q]\
\
def load_sales_yaml(path: str, Q: int) -> list[dict]:\
    with open(path, "r") as f:\
        y = yaml.safe_load(f) or \{\}\
    tiers = y.get("tiers", [])\
    packed = []\
    for t in tiers:\
        q = t["quarter"]\
        if isinstance(q, str) and "-" in q:\
            a, b = q.split("-")\
            qs = list(range(int(a), int(b)+1))\
        else:\
            qs = [int(q)]\
        amt = int(t["amount_sait"])\
        price = t.get("price_usd", None)\
        prange = t.get("price_usd_range", None)\
        if prange:\
            lo, hi = float(prange[0]), float(prange[1])\
            price = (lo + hi) / 2.0  # choose mid by default; you can parametrize\
        assert price is not None\
        for qi in qs:\
            if 1 <= qi <= Q:\
                packed.append(\{"quarter": qi, "amount_sait": amt // len(qs), "price_usd": float(price)\})\
    return packed\
\
