def main() -> None:\
    ap = argparse.ArgumentParser(description="SAIT Tokenomics Simulator")\
    ap.add_argument("--config", default="config.yaml", help="Path to config.yaml")\
    ap.add_argument("--schedule", default="unlock_schedule.csv", help="Path to unlock schedule CSV (percents of total)")\
    ap.add_argument("--phase_yaml", help="Path to phase YAML to auto-generate a cap-compliant schedule CSV")\
    ap.add_argument("--price_csv", help="Path to custom per-quarter price CSV (Price_USD column)")\
    ap.add_argument("--outdir", default="outputs", help="Directory to write outputs")\
    ap.add_argument("--verbose", action="store_true", help="Print inputs and schedule diagnostics")\
    args = ap.parse_args()\
\
    cfg = load_inputs(args.config)\
    Q = int(cfg["quarters"])\
    quarterly_cap_pct = 3.0 * float(cfg["max_unlock_cap_per_month"])\
    os.makedirs(args.outdir, exist_ok=True)\
\
    # Schedule: from phases or from CSV\
    if args.phase_yaml:\
        sched_df = generate_schedule_from_phases(args.phase_yaml, Q, quarterly_cap_pct)\
        # enforce global cap across overlapping phases\
        sched_df = enforce_global_quarterly_cap(sched_df, quarterly_cap_pct)\
        schedule_path = os.path.join(args.outdir, "derived_unlock_schedule.csv")\
        sched_df.to_csv(schedule_path, index=False)\
    else:\
        schedule_path = args.schedule\
        sched_df = load_schedule_csv(schedule_path)\
\
    # Price series\
    price_series = None\
    if args.price_csv:\
        price_series = load_price_csv(args.price_csv, Q)\
        price_mode = f"custom CSV (\{len(price_series)\} rows)"\
    else:\
        price_mode = f"geometric (Q1=$\{cfg['price_q1_usd']:.2f\}, QoQ=\{cfg['price_growth_qoq']*100:.1f\}%)"\
\
    # Verbose inputs & schedule diagnostics\
    if args.verbose:\
        stats = compute_schedule_stats(sched_df, quarterly_cap_pct)\
        print_inputs_summary(cfg, price_mode, stats)\
\
    # Run supplies\
    supplies = cfg.get("total_supply_grid", [])\
    if not supplies:\
        raise ValueError("No total_supply_grid in config.yaml")\
\
    kpis_all = []\
    for s in supplies:\
        out = simulate_one_supply(int(s), cfg, sched_df, price_series)\
\
        # 1) Save the full quarterly ledger\
        out.to_csv(os.path.join(args.outdir, f"results_\{int(s)\}.csv"), index=False)\
\
        # 2) Compute KPIs for THIS scenario\
        kpis = kpis_from_out(out, int(s), cfg, sched_df, quarterly_cap_pct)\
\
        # 3) Append to the summary list\
        kpis_all.append(kpis)\
\
        # 4) Per-scenario narrative report (use the 'kpis' we just computed)\
        buckets_16q = bucket_totals_pct(sched_df)\
        report = scenario_report_md(int(s), cfg, sched_df, out, kpis, buckets_16q, warn_overhang=0.40)\
        with open(os.path.join(args.outdir, f"report_\{int(s)\}.md"), "w") as f:\
            f.write(report)\
\
    # KPI summary\
    kdf = pd.DataFrame(kpis_all)\
    kdf.to_csv(os.path.join(args.outdir, "kpis_summary.csv"), index=False)\
    pretty_print_kpis(kpis_all)\
\
    # Markdown summary\
    lines = [\
        "# SAIT Tokenomics \'97 KPI Summary",\
        "",\
        "| Total Supply | Init Float | Final Circ | Cum Burn (SAIT) | Cum Burn % | Deflationary Qtrs | Min Runway Qtrs | Peak Unlock %/q | Max Overhang/Float | Cap Breach |",\
        "|---:|---:|---:|---:|---:|---:|---:|---:|---:|:---:|",\
    ]\
    for k in kpis_all:\
        runway = k["min_runway_quarters"]\
        runway_str = "\uc0\u8734 " if (isinstance(runway, float) and not math.isfinite(runway)) else int(runway)\
        lines.append(\
            f"| \{k['total_supply']:,\} | \{k['initial_float']:,\} | \{k['final_circulating']:,\} | \{k['cum_burn_sait']:,\} | "\
            f"\{k['cum_burn_pct_total']*100:0.2f\}% | \{k['deflationary_quarters']\} | \{runway_str\} | "\
            f"\{k['peak_unlock_pct_per_q']*100:0.2f\}% | \{k['max_overhang_vs_float']*100:0.1f\}% | "\
            f"\{'YES' if k['cap_breach_any'] else 'no'\} |"\
        )\
    with open(os.path.join(args.outdir, "kpis_summary.md"), "w") as f:\
        f.write("\\n".join(lines))\
\
    # Run manifest (reproducibility)\
    manifest = \{\
        "config": cfg,\
        "price_mode": price_mode,\
        "used_schedule_path": schedule_path,\
        "used_price_csv": args.price_csv or None,\
        "quarterly_cap_pct": quarterly_cap_pct,\
        "schedule_totals_pct_per_q": sched_df[BUCKETS].sum(axis=1).round(6).tolist(),\
        "total_supply_grid": [int(s) for s in supplies],\
    \}\
    with open(os.path.join(args.outdir, "run_manifest.json"), "w") as f:\
        json.dump(manifest, f, indent=2)\
\
    # --- Consolidated Markdown report ---\
    comp_lines = []\
    comp_lines.extend(plain_english_intro(cfg, supplies, price_mode))\
    comp_lines.append("# SAIT Tokenomics \'97 Consolidated Report")\
    comp_lines.append("")\
    comp_lines.append("## Executive Summary")\
    comp_lines.append("- This report compares total-supply scenarios using the same cap-compliant unlock schedule, fee/burn settings, and price path.")\
    comp_lines.append("- Burns scale with **usage** rather than supply, so SAIT burned is similar across caps; burn **percent** falls as supply grows.")\
    comp_lines.append("- Overhang/Float and Treasury runway are the two most important operational risk flags.")\
    comp_lines.append("")\
\
    # Inputs summary\
    comp_lines.append("## Inputs & Assumptions")\
    comp_lines.append(f"- Horizon: **\{cfg['quarters']\} quarters**")\
    comp_lines.append(f"- Total-supply grid: **\{', '.join(f'\{int(s):,\}' for s in supplies)\}**")\
    comp_lines.append(f"- Initial float: **\{cfg['initial_circulating_pct']*100:.2f\}%** of total (Q1 start)")\
    comp_lines.append(f"- Unlock cap: **\{cfg['max_unlock_cap_per_month']*100:.2f\}%/mo \uc0\u8658  \{3*cfg['max_unlock_cap_per_month']*100:.2f\}%/qtr** (global)")\
    comp_lines.append(f"- Usage: actions Q1=**\{cfg['base_actions_q1']:,\}**, growth QoQ=**\{cfg['actions_growth_qoq']*100:.1f\}%**")\
    comp_lines.append(f"- Fees: **$\{cfg['fee_per_action_usd']:.2f\}/action** | Splits: burn=\{cfg['burn_share']:.2f\}, stakers=\{cfg['stakers_share']:.2f\}, treasury=\{cfg['treasury_share']:.2f\}")\
    comp_lines.append(f"- Staking: %staked=**\{cfg['staked_pct_of_circ']*100:.1f\}%** | target APY=**\{cfg['target_staker_apy']*100:.1f\}%** (fee-funded cap)")\
    comp_lines.append(f"- Treasury: start=**$\{cfg['starting_treasury_cash_usd']:,\}**, ops=**$\{cfg['ops_spend_per_q_usd']:,\}/qtr**")\
    comp_lines.append(f"- Burn floor: **\{cfg.get('burn_floor_sait_per_q',0):,.0f\} SAIT/q** (\{cfg.get('burn_floor_finance','treasury')\})")\
    comp_lines.append(f"- Price: **\{price_mode\}**")\
    comp_lines.append("")\
\
    # Schedule diagnostics\
    sched_totals_pct = sched_df[BUCKETS].sum(axis=1)\
    comp_lines.append("## Schedule Diagnostics")\
    comp_lines.append(f"- Total unlock over 16q: **\{sched_totals_pct.sum()*100:.2f\}%** of total supply")\
    comp_lines.append(f"- Avg unlock per quarter: **\{sched_totals_pct.mean()*100:.2f\}%** | Max per quarter: **\{sched_totals_pct.max()*100:.2f\}%**")\
    comp_lines.append("- Global cap breaches: **none** (excess spilled forward if needed)")\
    comp_lines.append("")\
    comp_lines.append("### Unlocks by Bucket (16q totals)")\
    for b in BUCKETS:\
        comp_lines.append(f"- \{b\}: **\{sched_df[b].sum()*100:.2f\}%**")\
    comp_lines.append(f"- Total: **\{sched_totals_pct.sum()*100:.2f\}%**")\
    comp_lines.append("")\
\
    # Comparison table\
    comp_lines.append("## Scenario Comparison (Key KPIs)")\
    comp_lines.append("| Total Supply | Init Float | Final Circ | Final Circ % | Cum Burn (SAIT) | Cum Burn % | Defl Qtrs | Runway Q (min) | Peak Unlock %/q | Max Overhang/Float | Avg Staker APY |")\
    comp_lines.append("|---:|---:|---:|---:|---:|---:|---:|---:|---:|---:|---:|")\
    for k in kpis_all:\
        comp_lines.append(\
            f"| \{k['total_supply']:,\} | \{k['initial_float']:,\} | \{k['final_circulating']:,\} | \{k['final_circ_pct']\} | "\
            f"\{k['cum_burn_sait']:,\} | \{k['cum_burn_pct_total']*100:0.2f\}% | \{k['deflationary_quarters']\} | \{k['min_runway_quarters']\} | "\
            f"\{k['peak_unlock_pct_per_q']*100:0.2f\}% | \{k['max_overhang_vs_float']*100:0.1f\}% | \{k['avg_staker_apy']*100:0.2f\}% |"\
        )\
    comp_lines.append("")\
\
    # Recommendations\
    comp_lines.append("## Recommendations")\
    for r in consolidated_recommendations(kpis_all):\
        comp_lines.append(f"- \{r\}")\
    comp_lines.append("")\
\
    # Append each per-scenario report (already generated above)\
    comp_lines.append("## Per-Scenario Details")\
    for s in supplies:\
        p = os.path.join(args.outdir, f"report_\{int(s)\}.md")\
        if os.path.exists(p):\
            comp_lines.append(f"\\n---\\n\\n### Scenario \'97 Total Supply \{int(s):,\}\\n")\
            with open(p, "r") as rf:\
                # skip the first H1, keep the rest\
                lines_md = rf.read().splitlines()\
                body = [ln for ln in lines_md if not ln.startswith("# SAIT Scenario")]\
                comp_lines.extend(body)\
                comp_lines.append("")\
\
    with open(os.path.join(args.outdir, "SAIT_Tokenomics_Report.md"), "w") as f:\
        f.write("\\n".join(comp_lines))\
\
\
if __name__ == "__main__":\
    main()\
